{
	"name": "Cloudformation CLI",
	"dockerFile": "Dockerfile",
	"runArgs": [ "--cap-add=SYS_PTRACE", "--security-opt", "seccomp=unconfined" ],
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash",
		"go.gopath": "/go",
		"go.inferGopath": true,
		"go.useLanguageServer": true
	},
	"remoteUser": "vscode",
	"extensions": [
		"ms-vscode.go"
	]
}